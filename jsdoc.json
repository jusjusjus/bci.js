{
	"recurseDepth": 10,
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
    },
	"source": {
		"include": [],
		"exclude": [],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"markdown": {
		"excludeTags": ["author"]
	},
	"templates": {
		"referenceTitle": "BCI.js",
		"disableSort": false,
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./docs/docs/",
		"encoding": "utf8",
		"private": false,
		"recurse": true,
		"template": "./website/jsdoc-template"
    }
}
